define tablek1_bigg(kappa){
# Lawden 6.16.2 -- G = g_2^3 / (27 g_3^2) -- (g2w1^4)^3 / (27 (g3w1^6)^2)
  auto num,den;
  num=tablek1_g2w1fourth(kappa)^3;
  den=27*tablek1_g3w1sixth(kappa)^2;
  if(den!=0){
    return num/den;
  }else{
    if(num>0){print "+oo";}
    if(num<0){print "-oo";}
    if(num==0){print "NaN";}
    return 0;
  }
}

