define bigg2nu_g3gt0(g){
  auto gt,lo,hi,mid;
  /* Solving G = tablek2_bigg(nu) for g3>0 */
  /* Via bisection method */
  lo=1.0; hi=10.0;mid=(lo+hi)/2;
  while(((hi-mid)*(mid-lo)) >0){
    gt=tablek2_bigg(mid);
    if(g>gt){lo=mid;}else{hi=mid;}
    mid=(lo+hi)/2;
  }
  return mid;  
}
define bigg2nu_g3lt0(g){
  auto gt,lo,hi,mid;
  /* Solving G = tablek2_bigg(nu) for g3<0 */
  /* Via bisection method */
  lo=0.0; hi=1.0;mid=(lo+hi)/2;
  while(((hi-mid)*(mid-lo)) >0){
    gt=tablek2_bigg(mid);
    if(g<=gt){lo=mid;}else{hi=mid;}
    mid=(lo+hi)/2;
  }
  return mid;  
}
